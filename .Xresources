Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

URxvt.font: xft:consolas for powerline :size=11

Urxvt*dynamicColors:      true 
URxvt*utf8:               2 
URxvt*eightBitInput:      true 
URxvt*saveLines:          512 
URxvt*scrollKey:          true 
URxvt*scrollTtyOutput:    false 
URxvt*scrollBar:          false  
URxvt*rightScrollBar:     true 
URxvt*jumpScroll:         true 
URxvt*multiScroll:        true 
URxvt*toolBar:            false

Urxvt.scrollBar: false
URxvt*keysym.Home: \033[1~
URxvt*keysym.End: \033[4~


!Real Transparency: Using desktop composition
!urxvt*depth: 32
!urxvt*background: rgba:0000/0000/0200/c800

!Native Transparency
!URxvt*.transparent: true
! URxvt*.shading: 0 to 99 darkens, 101 to 200 lightens
!URxvt*.shading: 60
!URxvt*fading: 50

!URxvt.iconFile:    /usr/share/icons/Faenza/apps/48/gnome-terminal.png

!TomorrowNight
#define t_background        #1d1f21
#define t_current_line      #282a2e
#define t_selection             #373b41
#define t_foreground            #c5c8c6
#define t_comment               #969896
#define t_red                   #cc6666
#define t_orange                #de935f
#define t_yellow                #f0c674
#define t_green                 #b5bd68
#define t_aqua                  #8abeb7
#define t_blue                  #81a2be
#define t_purple                #b294bb


*.foreground:   t_foreground
*.background:   t_background
*.cursorColor:  #aeafad

! Black / Grey
*.color0:       #000000
*.color8:       #666666

! Red / Bright Red
*.color1:       t_red
*.color9:       #FF3334

! Green + Bright Green
*.color2:       t_green
*.color10:      #9ec400

! Yellow (Orange) + Bright Yellow (Yellow)
*.color3:       t_orange
*.color11:      t_yellow

! Blue + Bright Blue
*.color4:       t_blue
*.color12:      t_blue

! Magenta (Purple) + Bright Magenta
*.color5:       t_purple
*.color13:      #b777e0

! Cyan (Aqua) + Bright Cyan
*.color6:       t_aqua
*.color14:      #54ced6

! Light Grey (Selection) + White (Current Line)
*.color7:       t_selection
*.color15:      t_current_line
